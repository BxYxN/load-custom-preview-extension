{
    "name": "load-custom-preview",
    "displayName": "load-custom-preview",
    "description": "visualizaci√≥n de paginas espesificas",
    "version": "0.0.1",
    "publisher": "load-custom-preview",
    "repository": {
        "type": "git",
        "url": "https://github.com/BxYxN/load-custom-preview-extension"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.load-preview.on"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.load-preview.on",
                "title": "Load server view"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Config server preview",
            "properties": {
                "loadpreview.port": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "default": null,
                    "description": "port to load page"
                },
                "loadpreview.path": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "default": "/",
                    "description": "path to load page"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vsce": "^1.46.0",
        "vscode": "^1.1.6"
    }
}
